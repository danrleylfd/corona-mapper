{
  "expo": {
    "name": "CoronaMapper",
    "slug": "CoronaMapper",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "userInterfaceStyle": "automatic",
    "android": {
      "package": "com.mobile.coronamapper",
      "versionCode": 1,
      "config": {
        "googleMaps": { "apiKey": "<API KEY>" }   
      },
      "userInterfaceStyle": "dark"
    },
    "ios": {
      "bundleIdentifier": "com.mobile.coronamapper",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "<API KEY>"
      },
      "userInterfaceStyle": "light"
    }
  }
}
